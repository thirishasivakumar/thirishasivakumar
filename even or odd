import java.util.*;
class Sample{
    public static void main(String [] args){
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter an integer:");
        int N=sc.nextInt();

        if((N%2)==0)
        System.out.println("Num is Even.");
        else
        System.out.println("Num is Odd.");
    }
}
