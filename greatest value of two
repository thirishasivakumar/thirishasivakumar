import java.util.*;
class Sample{
    public static void main(String [] args){
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter an integer a:");
        int a=sc.nextInt();
        System.out.print("Enter an integer b:");
        int b=sc.nextInt();

        if(a>b)
        System.out.println("a is greater.");
        else
        System.out.println("b is greater.");
    }
}
