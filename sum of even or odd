import java.util.*;
class Sample{
    public static void main(String [] args){
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter an integer a:");
        int a=sc.nextInt();
        System.out.print("Enter an integer b:");
        int b=sc.nextInt();
        int c=a+b;

        if((c%2)==0)
        System.out.println("Sum is Even.");
        else
        System.out.println("Sum is Odd.");
    }
}
